          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_ros_rplidar_controller">
        <topicName>front/scan</topicName>
        <frameName>laser_oscillator</frameName>
        <!--<frameName>front_laser</frameName>-->
      </plugin>
    </sensor>
